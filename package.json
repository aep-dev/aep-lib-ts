{
  "name": "aep-lib-ts",
  "version": "1.0.0",
  "description": "Utility libraries for AEP TypeScript-based tools including case conversion, OpenAPI utilities, and API clients",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "dev": "tsc --watch"
  },
  "keywords": [
    "aep",
    "openapi",
    "typescript",
    "case-conversion",
    "api-client"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "winston": "^3.11.0",
    "axios": "^1.6.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/js-yaml": "^4.0.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0",
    "yargs": "^17.7.0",
    "nock": "^13.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}